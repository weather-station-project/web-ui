{
  "name": "web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/weather-station-project/web-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --build && vite build",
    "lint": "eslint src --max-warnings 0 --fix",
    "format": "prettier src --write",
    "preview": "vite preview",
    "prepare": "husky && bash -c 'chmod ug+x .husky/*'",
    "cy:open": "cypress open",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-comp": "cypress run --component",
    "cy:test-e2e": "start-server-and-test dev http-get://localhost:5173 cy:run-e2e"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-zone": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/instrumentation-document-load": "^0.48.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.48.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.203.0",
    "@opentelemetry/opentelemetry-browser-detector": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@tanstack/react-query": "^5.84.1",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "bootstrap": "^5.3.7",
    "classnames": "^2.5.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.7.1",
    "retry": "^0.13.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/retry": "^0.12.5",
    "@vitejs/plugin-react": "^4.7.0",
    "cypress": "^14.5.3",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.13",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6"
  }
}
