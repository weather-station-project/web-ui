{
  "name": "web-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/weather-station-project/web-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --build && vite build",
    "lint": "eslint src --max-warnings 0 --fix",
    "format": "prettier src --write",
    "preview": "vite preview",
    "prepare": "husky && bash -c 'chmod ug+x .husky/*'",
    "cy:open": "cypress open",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-comp": "cypress run --component",
    "cy:test-e2e": "start-server-and-test dev http-get://localhost:5173 cy:run-e2e",
    "cy:test-comp": "start-server-and-test dev http-get://localhost:5173 cy:run-comp"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.2.0",
    "retry": "^0.13.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/retry": "^0.12.5",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.2.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
